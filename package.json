    "@types/react-transition-group": "^4.4.12",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "gsap": "^3.12.7",
    "lucide-react": "^0.474.0",
    "next": "15.0.0",
    "payload": "beta",
    "react": "19.0.0-rc-65a56d0e-20241020",
